    <script>
      
  const maxFiles =10;  // Maximum number of files allowed

    document.getElementById('file-input').addEventListener('change', function(event) {
    var fileInput = document.getElementById('file-input');
    var fileCount = fileInput.files.length;
    var errorMessage = document.getElementById('errorMessage');
    
    if (fileCount > maxFiles) {
        errorMessage.textContent = "You can only select up to " + maxFiles + " files.";
        fileInput.value = "";  // Clear the input
        document.getElementById('fileCount').textContent = "No files selected";
        
    } else {
        errorMessage.textContent = "";
        document.getElementById('fileCount').textContent = fileCount + " file(s) selected";
    }
});

    </script>

